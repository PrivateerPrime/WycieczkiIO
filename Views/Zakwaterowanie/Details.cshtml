@model WycieczkiIO.Models.Zakwaterowanie

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Details</h1>

<div>
    <h4>Zakwaterowanie</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Adres)
        </dt>
        <dd class = "col-sm-10">
            Ulica: @ViewData["Ulica"]
            Numer: @ViewData["Numer"]
            Kod pocztowy: @ViewData["KodPocztowy"]
            Miasto: @ViewData["NazwaMiasta"]
            Kraj: @ViewData["NazwaKraju"]
        </dd>

        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nazwa)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nazwa)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Typ)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Typ)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ZakwaterowanieId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
